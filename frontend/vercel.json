{
  "version": 2,
  "buildCommand": "CI=false NODE_OPTIONS='--max-old-space-size=4096' yarn build",
  "outputDirectory": "build",
  "installCommand": "yarn install --frozen-lockfile",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "DISABLE_ESLINT_PLUGIN": "true"
    }
  }
}
